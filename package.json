    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "vite": "^5.1.0",
    "vitest": "^3.2.4"
